<ul>
    <% @apartments.each do |apartment| %>
    <li>
        <%= link_to apartment.apartment_name, apartment %>
    </li>
    <% end %>
</ul>
<br>


<nav class="navbar navbar-expand-lg navbar-light bg-light">

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <form class="form-inline my-2 my-lg-0">

    <%= form_with model: @apartment do |form| %>
    <div>
        <%= form.label 'search' %><br>
        <%= form.search_field :search %>
    </div>
    <%= form.submit 'Search', method: :get %>
    <% end %>

    </form>
  </div>
</nav>

<%= button_to 'press', list_path, method: :get %>

<br>
<br>
<br>

<%= link_to "New Apartment", new_apartment_path %>


<%if !(params[:search].present?) %>
  <%= paginate @apartments %>
<% end %>


